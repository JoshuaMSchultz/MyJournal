<%= div_for example do %> 
  <%= example.body %><br>
  <%= link_to 'Edit Example', edit_thought_example_path(@thought, example) %>
  <%= link_to 'Delete Example', thought_example_path(:thought_id => @thought, :id => example), confirm: 'Are you sure?', method: :delete %>
  <%= render :file => 'sources/_source',  locals: { examples: @thought.examples }  %>
<% end %>
